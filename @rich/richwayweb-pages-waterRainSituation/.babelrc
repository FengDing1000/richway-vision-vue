{
    "ignore": [
        "**/*.min.js"
    ],
    "presets": [
        [
            "env",
            {
                "modules": false,
                "loose": true,
                "useBuiltIns": false,
                "targets": {
                    "browsers": [
                        "last 4 versions",
                        "ie >= 9",
                        "iOS >= 7",
                        "Android >= 4"
                    ]
                }
            }
        ],
        "stage-2"
    ],
    "comments": false,
    "plugins": [
        "transform-runtime",
        "syntax-dynamic-import",
        "dynamic-import-webpack",
        [
            "component",
            {
                "libraryName": "element-ui",
                "styleLibraryName": "theme-chalk"
            }
        ]
    ]
}
